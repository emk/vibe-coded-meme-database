{
  "name": "meme-db",
  "version": "1.0.0",
  "description": "A single-user database for storing, indexing and searching meme images",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "serve": "ts-node src/index.ts",
    "import": "ts-node src/scripts/import.ts",
    "analyze": "ts-node src/scripts/analyze.ts",
    "clear-db": "ts-node src/scripts/clear-db.ts",
    "update-schema": "typescript-json-schema --required src/services/meme-analysis.ts MemeAnalysis -o src/services/meme-analysis.json", 
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "memes",
    "database",
    "ollama",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "better-sqlite3": "^11.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ollama": "^0.5.14",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "typescript-json-schema": "^0.65.1"
  }
}
